async function fetchGetText(url)    //функция получения текста по ссылке
{
  try 
  {
    var response = await fetch(url);
    var answer = await response.text();
    var result = answer.toString().split(';'); //делаем массив по разделителю
    return result;
  } 
  catch (error) 
  {
    console.error(error);
  }
}

