function logicBotik(msg, name) //основная логика ботика
{
  aaa = msg + ' ' + name;
  fetch('https://raw.githubusercontent.com/Ssid113/nightbot/master/botik/all_commands')
    .then(response => response.text())
    .then(answer => answer.split(';'))
    .then(result => nextfunc(result));
};

function nextfunc(result)
{
  test = result;
  msg = test[1].toString();
};

logicBotik(msg, name);
